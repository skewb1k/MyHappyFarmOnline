<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <ImageView
            android:id="@+id/animalmarket0"
            android:layout_width="50dp"
            android:layout_height="75dp"
            android:onClick="animalmarketClicked"
            android:src="@drawable/pig_2"/>
    <ImageView
            android:id="@+id/animalmarket0_selection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/animal_selection"
            app:layout_constraintLeft_toLeftOf="@id/animalmarket0"
            app:layout_constraintTop_toTopOf="@id/animalmarket0"/>
    <ImageView
            android:id="@+id/animalmarket1"
            android:layout_width="50dp"
            android:layout_height="75dp"
            android:onClick="animalmarketClicked"
            android:layout_marginLeft="9dp"
            app:layout_constraintLeft_toRightOf="@id/animalmarket0"
            android:src="@drawable/pig_2"/>
    <ImageView
            android:id="@+id/animalmarket1_selection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/animal_selection"
            app:layout_constraintLeft_toLeftOf="@id/animalmarket1"
            app:layout_constraintTop_toTopOf="@id/animalmarket1"/>
    <ImageView
            android:id="@+id/animalmarket2"
            android:layout_width="50dp"
            android:layout_height="75dp"
            android:onClick="animalmarketClicked"
            android:layout_marginLeft="9dp"
            app:layout_constraintLeft_toRightOf="@id/animalmarket1"
            android:src="@drawable/pig_2"/>
    <ImageView
            android:id="@+id/animalmarket2_selection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/animal_selection"
            app:layout_constraintLeft_toLeftOf="@id/animalmarket2"
            app:layout_constraintTop_toTopOf="@id/animalmarket2"/>
    <ImageView
            android:id="@+id/animalmarket3"
            android:layout_width="50dp"
            android:layout_height="75dp"
            android:onClick="animalmarketClicked"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@id/animalmarket0"
            android:src="@drawable/pig_2"/>
    <ImageView
            android:id="@+id/animalmarket3_selection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/animal_selection"
            app:layout_constraintLeft_toLeftOf="@id/animalmarket3"
            app:layout_constraintTop_toTopOf="@id/animalmarket3"/>
    <ImageView
            android:id="@+id/animalmarket4"
            android:layout_width="50dp"
            android:layout_height="75dp"
            android:onClick="animalmarketClicked"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@id/animalmarket1"
            app:layout_constraintLeft_toRightOf="@id/animalmarket3"
            android:src="@drawable/pig_2"/>
    <ImageView
            android:id="@+id/animalmarket4_selection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/animal_selection"
            app:layout_constraintLeft_toLeftOf="@id/animalmarket4"
            app:layout_constraintTop_toTopOf="@id/animalmarket4"/>
    <ImageView
            android:id="@+id/animalmarket5"
            android:layout_width="50dp"
            android:layout_height="75dp"
            android:onClick="animalmarketClicked"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@id/animalmarket2"
            app:layout_constraintLeft_toRightOf="@id/animalmarket4"
            android:src="@drawable/pig_2"/>
    <ImageView
            android:id="@+id/animalmarket5_selection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/animal_selection"
            app:layout_constraintLeft_toLeftOf="@id/animalmarket5"
            app:layout_constraintTop_toTopOf="@id/animalmarket5"/>
</androidx.constraintlayout.widget.ConstraintLayout>